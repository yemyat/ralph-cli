{
  "name": "ralph-wiggum-cli",
  "version": "1.0.6",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ralph-wiggum-cli": "./bin/ralph-wiggum-cli.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "lint": "ultracite check",
    "lint:fix": "ultracite fix",
    "format": "ultracite fix",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist",
    "bin"
  ],
  "keywords": [
    "ralph",
    "ai",
    "agent",
    "claude",
    "amp",
    "droid",
    "opencode",
    "cursor",
    "codex",
    "gemini"
  ],
  "author": "Ye Myat Min (Jeff)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yemyat/ralph-cli.git"
  },
  "homepage": "https://github.com/yemyat/ralph-cli#readme",
  "bugs": {
    "url": "https://github.com/yemyat/ralph-cli/issues"
  },
  "description": "CLI tool for managing Ralph Wiggum AI development workflows",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@opentui/core": "^0.1.73",
    "@opentui/react": "^0.1.73",
    "picocolors": "^1.1.1",
    "commander": "^14.0.2",
    "fs-extra": "^11.3.3",
    "ora": "^9.0.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "^1.3.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.8",
    "react-devtools-core": "^7.0.1",
    "typescript": "^5.9.3",
    "ultracite": "7.0.11"
  }
}
